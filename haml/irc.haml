!!! XML
!!!
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}
    %meta{:http => {:equiv => "X-UA-Compatible"}, :content => "IE=edge"}
    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1"}
    %meta{:content => "", :name => "FSU CompClub website"}
    %meta{:content => "", :name => "author"}
    %title FSU CompClub
    %link{:rel => "icon", :href => "bootstrap/docs/favicon.ico"}
    
    / Bootstrap core CSS
    %link{:href => "bootstrap/dist/css/bootstrap.min.css", :rel => "stylesheet"}
    / Custom styles for fixing padding
    :css
      body {
        padding-top: 50px;
        padding-bottom: 20px;
      }
    / Just for debugging purposes. Don't actually copy these 2 lines!
    / [if lt IE 9] <script src="bootstrap/assets/js/ie8-responsive-file-warning.js"></script>
    %script{:src => "bootstrap/assets/js/ie-emulation-modes-warning.js"}
    / HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    / [if lt IE 9]
    %script{:src => "https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"}
    %script{:src => "https://oss.maxcdn.com/respond/1.4.2/respond.min.js"}

  %body
    %nav.navbar.navbar-inverse.navbar-fixed-top
      .container
        .navbar-header
          %button.navbar-toggle.collapsed{"aria-controls" => "navbar", "aria-expanded" => "false", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand{:href => "index.html"} FSU Computer Club
        #navbar.navbar-collapse.collapse
          %ul.nav.navbar-nav
            %li
              %a{:href => "index.html"} Home
            %li
              %a{:href => "events.html"} Events
            %li.active
              %a{:href => "#"} IRC Channel
        / /.navbar-collapse
      / Jumbotron
    .container

      %h1 IRC Channel
      %hr

      %iframe{:src => "https://kiwiirc.com/client/irc.rizon.net/#fsucompclub", :style => "border:0; width:100%; height:450px;"}
      
      %hr/

      %footer
        %p &copy; FSU Comp. Club 2015

    / Javascript
    %script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"}
    %script{:src => "bootstrap/dist/js/bootstrap.min.js"}
    %script{:src => "bootstrap/assets/js/ie10-viewport-bug-workaround.js"}
